    "@types/bcrypt": "5.0.2",
    "@types/node": "24.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.29.0",
    "globals": "16.2.0",
    "prisma": "^6.9.0",
    "tsup": "8.5.0",
    "tsx": "4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.34.1",
    "vite-tsconfig-paths": "5.1.4"
  },
  "dependencies": {
    "@fastify/jwt": "9.1.0",
