    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "postgres",
    "docker",
    "nodejs",
    "fastify",
    "vitest",
    "rest",
    "api",
    "backend",
    "gym",
    "system",
    "management"
  ],
  "author": "Marcio Gabriel Mengali",
  "license": "ISC",
